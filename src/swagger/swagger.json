          },
          "message": {
            "type": "string",
            "example": "Successfully registered new user account"
          }
        }
      },
      "Register_invalid": {
        "properties": {
          "success": {
            "type": "boolean",
            "example": false
          },
          "message": {
            "type": "string",
            "example": "Error while registering new user account"
          }
        }
      },
      "RegisterLogin_invalidation": {
        "properties": {
          "success": {
            "type": "boolean",
            "example": false
