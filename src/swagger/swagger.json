              "type": "integer"
            },
            "example": [20239, 34329]
          },
          "message": {
            "type": "string",
            "example": "Good Health"
          },
          "timestamp": {
            "type": "integer",
            "example": 2349249
          }
        }
      },
      "Login_success": {
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "token": {
            "type": "string",
            "example": "fkdslk23409ujf32490kcl239"
          },
          "data": {
            "properties": {
              "userid": {
                "type": "integer",
                "example": "12449023984"
              },
              "username": {
                "type": "string",
                "example": "jhondoe"
              },
              "email": {
                "type": "email",
                "example": "jhondoe@gmail.com"
              }
            }
          },
          "message": {
            "type": "string",
            "example": "Successfully LoggedIn"
          }
        }
      },
      "Login_invalid": {
        "properties": {
          "success": {
            "type": "boolean",
            "example": false
          },
          "message": {
            "type": "string",
            "example": "Error while login user account"
          }
        }
      },
      "Logout_success": {
        "properties": {
          "success": {
            "type": "boolean",
            "example": true
          },
          "message": {
            "type": "string",
            "example": "Successfully Logged out"
          }
